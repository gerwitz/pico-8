__lua__
-- 10print
-- @gerwitz

function _init()
 for x=0,15 do
  for y=0,15 do
   mset(x,y,rnd({1,2}))
  end
 end
end

function _update()
 x = flr( rnd(16) )
 y = flr( rnd(16) )
 current = mget(x,y)
 flipped = current % 2 + 1
 mset(x,y,flipped)
end

function _draw()
	cls()
	map(0,0,0,0,16,16)
end

